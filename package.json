{
  "name": "amare-api",
  "version": "1.0.0",
  "description": "Centralized API for Keap integration - newsletter signups and Clickbank IPN processing",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ckgmra/amare-api.git"
  },
  "keywords": [
    "keap",
    "infusionsoft",
    "clickbank",
    "api",
    "cloud-run"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ckgmra/amare-api/issues"
  },
  "homepage": "https://github.com/ckgmra/amare-api#readme",
  "dependencies": {
    "@fastify/cors": "^11.2.0",
    "@fastify/rate-limit": "^10.3.0",
    "@google-cloud/bigquery": "^8.1.1",
    "axios": "^1.13.2",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.2",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^25.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "eslint": "^9.39.1",
    "prettier": "^3.7.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
